package arida.ufc.br.moap.core.beans;

public class Pair<A,B> {

	private A typeA;
	private B typeB;
	
	public Pair(A typeA, B typeB){
		this.typeA = typeA;
		this.typeB = typeB;
	}
	
	public A getFirst(){
		return typeA;
	}
	
	public B getSecond(){
		return this.typeB;
	}
	
	@Override
	public String toString(){
		return "<"+typeA+", "+typeB+">";
	}
}
